        <script>

$(function () {
    var $form = $(".scholarship-subscription-form");

    $form.find("#subscribe-all").on("click", function() {
      var $this = $(this);
      if ($this.is(":checked")) {
        $form.find("input[type='checkbox']").not("#subscribe-all").prop('checked', true);
      } else {
        $form.find("input[type='checkbox']").not("#subscribe-all").prop('checked', false);
      }

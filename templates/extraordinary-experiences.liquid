<script>

  $(function() {
      var advActivities;
      var advMap;
      var $advAutocomplete = $(".adventure-autocomplete");
      var $advAutocompleteSelected = $(".adventure-activities-selected");
      var corElements;
      var corMap;
      var $corAutocomplete = $(".cor-autocomplete");
      var ttnElements;
      var ttnMap;
      var $ttnAutocomplete = $(".ttn-autocomplete");

      // Adventure Activities
      advActivities = $(".adventure-activities input[type='checkbox']");
      advMap = advActivities.map(function(i) {
          return {
            'id': $(this).attr('value'),
            'name': $(this).next('label').html()
          }
        })
        .get();
      $advAutocomplete.autocomplete(advMap)
        .on('itemChosen', function(e, data, textUserEnteredd, selectedListItem) {
          var $checkbox = advActivities.filter('input[value="' + data.id + '"]');
          $checkbox.prop('checked', true);

          var $liSelectedSpan = $('<span>')
            .html('x')
            .data("target-checkbox", $checkbox)
            .on('click', function() {
              $this = $(this);
              $this.data("target-checkbox").prop('checked', false);
              $this.parent("li").remove();
            });
          var $liSelected = $('<li>')
            .html($checkbox.next('label').html())
            .append($liSelectedSpan);

          $advAutocompleteSelected.append($liSelected);

          $(this).val('');
        });

      // Country of Residence
      corElements = $(".country-of-residence select option");
      corMap = corElements.map(function(i) {
          return {
            'id': $(this).attr('value'),
            'name': $(this).html()
          }
        })
        .get();
      $corAutocomplete.autocomplete(corMap)
        .on('itemChosen', function(e, data, textUserEnteredd, selectedListItem) {
          $(".country-of-residence select").val(data.id);
          $(this).val(data.name);
        });

      // Travelling to next
      ttnElements = $(".travelling-to-next select option");
      ttnMap = ttnElements.map(function(i) {
          return {
            'id': $(this).attr('value'),
            'name': $(this).html()
          }
        })
        .get();
      $ttnAutocomplete.autocomplete(ttnMap)
        .on('itemChosen', function(e, data, textUserEnteredd, selectedListItem) {
          $(".travelling-to-next select").val(data.id);
          $(this).val(data.name);
        });
  });

  // Datepicker for When are they travelling next
  $(function() {

      var dateNow = new Date();
      var dateNowDay = dateNow.getUTCDate();
      var dateNowMonth = dateNow.getUTCMonth() + 1;
      var dateNowYear = dateNow.getUTCFullYear();
      var $whenSelects = $(".when-are-they-travelling select");
      var $whenDay = $whenSelects.eq(1); // WTF why is it in this order
      var $whenMonth = $whenSelects.eq(0);
      var $whenYear = $whenSelects.eq(2);
      var $whenInput = $(".when-datepicker");

      $whenDay.val(dateNowDay);
      $whenMonth.val(dateNowMonth);
      $whenYear.val(dateNowYear);

      $whenInput.datePickerForSelects(
        $whenDay,
        $whenMonth,
        $whenYear
      );

  });


  // Are you travelling toggle
  $(function() {

      var $ttnDiv = $(".travelling-to-next").hide();
      var $wattDiv = $(".when-are-they-travelling").hide();
      var $tRadios = $(".travelling-radio");
      var $tRadioYes = $("#travelling-yes");
      var $tRadioNo = $("#travelling-no").prop("checked", true);

      $tRadios.on("change", function() {
        var $this = $(this);
        var targetId = $this.attr("id");
        if (targetId == "travelling-yes") {
          // YES
          $ttnDiv.show();
          $wattDiv.show();
        } else if (targetId == "travelling-no") {
          // NO
          $ttnDiv.hide();
          $wattDiv.hide();
        }
      });

  });


</script>


                                                          

